  <joint name="world_to_link0" type="fixed">
    <parent link="world" />
    <child link="$(arg arm_id)_link0" />
  </joint>


  <link name="stick">
    <visual>
      <geometry>
        <box size="0.15 0.025 0.025" />
      </geometry>
      <material name="blue_3">
        <color rgba="0 0 .8 0.5" />
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.13 0.025 0.025" />
      </geometry>
    </collision>
  </link>

  <joint name="panda_hand_to_stick" type="fixed">
    <parent link="panda_hand" />
    <child link="stick" />
    <!-- Lever -->
    <!-- <origin xyz="0.11 0.03 0.12" rpy="0 0 0.4" /> -->
    <!-- Stinger -->
    <origin xyz="0.115 0.0 0.11" rpy="0 0 0" />
  </joint>

