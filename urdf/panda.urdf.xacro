  <!-- <xacro:camera camera_name="camera1" x="0.57881" y="-0.45396" z="0.51466" /> -->
  <xacro:obstacle obstacle_name="rear_wall" x="-0.4" y="0" z="0.5" l="0.1" w="2.0" h="2.0"/>
  <xacro:obstacle obstacle_name="ceiling" x="1.0" y="0" z="1.25" l="2.5" w="3.0" h="0.1"/>
  <xacro:obstacle obstacle_name="left_wall" x="1.0" y="0.5" z="0.5" l="2.0" w="0.1" h="2.0"/>
  <xacro:obstacle obstacle_name="light_stand" x="-0.1" y="-0.6" z="0.5" l="0.1" w="0.1" h="1.0"/>
</robot>
